{
  "private": true,
  "scripts": {
    "bootstrap:cra": "vercel link apps/cra",
    "postbootstrap:cra": "(cd apps/cra && vercel env pull .env.local)",
    "bootstrap:next": "vercel link apps/next",
    "postbootstrap:next": "(cd apps/next && vercel env pull .env.local)",
    "bootstrap:remix": "vercel link apps/remix",
    "postbootstrap:remix": "(cd apps/remix && vercel env pull .env.local)",
    "bootstrap:studio": "vercel link apps/studio",
    "postbootstrap:studio": "(cd apps/studio && vercel env pull .env.local)",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --cache --write .",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:esm": "turbo run test:esm"
  },
  "devDependencies": {
    "prettier": "^2.8.5",
    "prettier-plugin-packagejson": "^2.4.3",
    "turbo": "^1.8.4",
