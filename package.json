{
  "name": "@sanity/preview-kit",
  "version": "0.0.0",
  "description": "General purpose live previews, like next-sanity",
  "keywords": [
    "sanity",
    "sanity.io",
    "next.js",
    "studio",
    "react",
    "preview",
    "groq",
    "live"
  ],
  "homepage": "https://github.com/sanity-io/preview-kit#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/preview-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/preview-kit.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "pkg build --tsconfig tsconfig.build.json --strict",
    "clean": "rimraf dist",
    "dev": "next",
    "format": "npm run prettier && eslint --cache --fix .",
    "lint": "eslint --max-warnings 0 .",
    "prepublishOnly": "npm run build",
    "prettier": "npx prettier --ignore-path .gitignore --cache --write .",
    "test": "node test.mjs && node test.cjs",
    "type-check": "tsc --noEmit"
  },
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/groq-store": "^1.0.3",
    "@types/event-source-polyfill": "^1.0.0",
    "event-source-polyfill": "^1.0.31",
    "suspend-react": "^0.0.8"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^2.0.1",
    "@sanity/pkg-utils": "^1.18.0",
    "@sanity/semantic-release-preset": "^2.0.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "bulma": "^0.9.4",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-gitignore": "^0.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "next": "13.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "^18.2.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "engines": {
    "node": ">=14"
  }
}
