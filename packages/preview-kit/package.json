{
  "name": "@sanity/preview-kit",
  "version": "2.4.0",
  "description": "General purpose utils for live content and visual editing",
  "keywords": [
    "groq",
    "live",
    "preview",
    "react",
    "sanity.io",
    "sanity",
    "studio",
    "svelte",
    "vue"
  ],
  "homepage": "https://github.com/sanity-io/preview-kit#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/preview-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/preview-kit.git",
    "directory": "packages/preview-kit"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "node": {
        "import": "./dist/index.cjs.js",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "source": "./src/client/index.ts",
      "require": "./dist/client.cjs",
      "node": {
        "import": "./dist/client.cjs.js",
        "require": "./dist/client.cjs"
      },
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./groq-store": {
      "types": "./dist/groq-store.d.ts",
      "source": "./src/groq-store/_legacy.ts",
      "require": "./dist/groq-store.cjs",
      "node": {
        "import": "./dist/groq-store.cjs.js",
        "require": "./dist/groq-store.cjs"
      },
      "import": "./dist/groq-store.js",
      "default": "./dist/groq-store.js"
    },
    "./csm": {
      "types": "./dist/csm.d.ts",
      "source": "./src/csm/index.ts",
      "require": "./dist/csm.cjs",
      "node": {
        "import": "./dist/csm.cjs.js",
        "require": "./dist/csm.cjs"
      },
      "import": "./dist/csm.js",
      "default": "./dist/csm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client.d.ts"
      ],
      "groq-store": [
        "./dist/groq-store.d.ts"
      ],
      "csm": [
        "./dist/csm.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "client.js",
    "groq-store.js",
    "csm.js"
  ],
  "scripts": {
    "prebuild": "rimraf dist coverage",
    "build": "pkg build --strict && pkg --strict",
    "watch": "pnpm build watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint --max-warnings 0 .",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release || semantic-release --dry-run --debug",
    "test": "vitest",
    "pretest:esm": "pnpm run build",
    "test:esm": "node test.mjs && node test.cjs"
  },
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/eventsource": "5.0.0",
    "@sanity/groq-store": "2.3.1",
    "@vercel/stega": "0.1.0",
    "lodash.get": "4.4.2",
    "lodash.isplainobject": "4.0.6",
    "lru-cache": "10.0.0",
    "mendoza": "2.1.1",
    "react-fast-compare": "3.2.2",
    "tiny-invariant": "1.3.1",
    "use-sync-external-store": "1.2.0"
  },
  "devDependencies": {
    "@sanity/client": "^6.4.0",
    "@sanity/pkg-utils": "^2.3.10",
    "@sanity/semantic-release-preset": "^4.1.2",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/node": "^18.17.2",
    "@types/react": "^18.2.18",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitest/coverage-c8": "^0.33.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-gitignore": "^0.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "peerDependencies": {
    "@sanity/client": "^6.4.0",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
