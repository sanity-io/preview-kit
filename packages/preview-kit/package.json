{
  "name": "@sanity/preview-kit",
  "version": "5.0.45",
  "description": "General purpose utils for live content and visual editing",
  "keywords": [
    "groq",
    "live",
    "preview",
    "react",
    "sanity.io",
    "sanity",
    "studio",
    "svelte",
    "vue"
  ],
  "homepage": "https://github.com/sanity-io/preview-kit#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/preview-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/preview-kit.git",
    "directory": "packages/preview-kit"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./client": {
      "source": "./src/client/index.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs",
      "default": "./dist/client/index.js"
    },
    "./live-query": {
      "source": "./src/live-query/index.ts",
      "import": "./dist/live-query/index.js",
      "require": "./dist/live-query/index.cjs",
      "default": "./dist/live-query/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ],
      "live-query": [
        "./dist/live-query/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf coverage dist || true",
    "build": "pkg build --strict && pkg --strict",
    "watch": "pnpm build watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint --max-warnings 0 .",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release || semantic-release --dry-run --debug",
    "test": "vitest"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/preview-kit-compat": "1.4.17",
    "mendoza": "3.0.7"
  },
  "devDependencies": {
    "@sanity/client": "^6.15.19",
    "@sanity/pkg-utils": "6.8.1",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@types/node": "^18.18.6",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/use-sync-external-store": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vercel/stega": "0.1.0",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-gitignore": "^0.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "lru-cache": "10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-compare": "3.2.2",
    "rimraf": "^5.0.1",
    "semantic-release": "^23.0.8",
    "typescript": "5.4.5",
    "use-sync-external-store": "1.2.0",
    "vitest": "^1.5.0",
    "vitest-github-actions-reporter": "^0.11.1"
  },
  "peerDependencies": {
    "@sanity/client": "^6.15.19",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
