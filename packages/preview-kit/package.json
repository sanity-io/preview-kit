{
  "name": "@sanity/preview-kit",
  "version": "2.0.0",
  "description": "General purpose utils for live content and visual editing",
  "keywords": [
    "groq",
    "live",
    "preview",
    "react",
    "sanity.io",
    "sanity",
    "studio",
    "svelte",
    "vue"
  ],
  "homepage": "https://github.com/sanity-io/preview-kit#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/preview-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/preview-kit.git",
    "directory": "packages/preview-kit"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "node": {
        "import": "./dist/index.cjs.js",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "source": "./src/client/index.ts",
      "require": "./dist/client.cjs",
      "node": {
        "import": "./dist/client.cjs.js",
        "require": "./dist/client.cjs"
      },
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./groq-store": {
      "types": "./dist/groq-store.d.ts",
      "source": "./src/groq-store/index.ts",
      "require": "./dist/groq-store.cjs",
      "node": {
        "import": "./dist/groq-store.cjs.js",
        "require": "./dist/groq-store.cjs"
      },
      "import": "./dist/groq-store.js",
      "default": "./dist/groq-store.js"
    },
    "./live-store": {
      "types": "./dist/live-store.d.ts",
      "source": "./src/live-store/index.ts",
      "require": "./dist/live-store.cjs",
      "node": {
        "import": "./dist/live-store.cjs.js",
        "require": "./dist/live-store.cjs"
      },
      "import": "./dist/live-store.js",
      "default": "./dist/live-store.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client.d.ts"
      ],
      "groq-store": [
        "./dist/groq-store.d.ts"
      ],
      "live-store": [
        "./dist/live-store.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "client.js",
    "groq-store.js",
    "live-store.js"
  ],
  "scripts": {
    "prebuild": "rimraf dist coverage",
    "build": "pkg build --strict && pkg --strict",
    "watch": "pnpm build watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint --max-warnings 0 .",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release || semantic-release --dry-run --debug",
    "test": "vitest",
    "pretest:esm": "pnpm run build",
    "test:esm": "node test.mjs && node test.cjs"
  },
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/eventsource": "^5.0.0",
    "@sanity/groq-store": "2.1.0",
    "@vercel/stega": "0.0.5",
    "groq": "^3.0.0",
    "lodash.get": "4.4.2",
    "lodash.isplainobject": "4.0.6",
    "lru-cache": "9.1.2",
    "mendoza": "2.1.1",
    "react-fast-compare": "3.2.2",
    "tiny-invariant": "1.3.1",
    "use-sync-external-store": "1.2.0"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "2.3.2",
    "@sanity/semantic-release-preset": "4.1.1",
    "@types/lodash.get": "4.4.7",
    "@types/lodash.isplainobject": "4.0.7",
    "@types/node": "18.16.16",
    "@types/react": "18.2.9",
    "@types/use-sync-external-store": "0.0.3",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "@vitest/coverage-c8": "0.32.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-gitignore": "0.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.3",
    "typescript": "5.1.3",
    "vitest": "0.32.0",
    "vitest-github-actions-reporter": "0.10.0"
  },
  "peerDependencies": {
    "@sanity/client": "^6.1.1",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
